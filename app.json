{
	"expo": {
		"name": "econecta",
		"slug": "econecta",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "econecta",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/android-foreground.png",
				"monochromeImage": "./assets/images/android-foreground.png",
				"backgroundImage": "./assets/images/android-background.png"
			},
			"config": {
				"googleMaps": {
					"apiKey": "AIzaSyAADgIQH8u401bUJO0sujYAsGmrgDaLiR0"
				}
			},
			"edgeToEdgeEnabled": true,
			"predictiveBackGestureEnabled": false,
			"package": "com.econecta.android"
		},
		"androidStatusBar": {
			"barStyle": "dark-content"
		},
		"androidNavigationBar": {
			"enforceContrast": false
		},
		"web": {
			"name": "econecta",
			"output": "static",
			"favicon": "./assets/images/favicon.png",
			"bundler": "metro"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#F6F7EB",
					"dark": {
						"backgroundColor": "#F6F7EB"
					}
				}
			],
			[
				"expo-maps",
				{
					"requestLocationPermission": true,
					"locationPermission": "Allow $(PRODUCT_NAME) to use your location"
				}
			],
			[
				"expo-camera",
				{
					"cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
					"microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
					"recordAudioAndroid": true
				}
			],
			"expo-web-browser",
			"expo-font",
			"expo-secure-store"
		],
		"experiments": {
			"tsconfigPaths": true,
			"typedRoutes": true,
			"reactCompiler": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "3cadabf5-4838-4a04-996c-df4ed5c9c097"
			}
		},
		"owner": "meninocoiso"
	}
}
